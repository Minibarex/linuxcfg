#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias nigga="echo 'ooga booga'"
alias wifi='sudo nmtui'
alias bashconf='nano ~/.bashrc'
alias winmount='sudo mount /dev/sda2 windows'
alias shutdown='shutdown +0'
PS1='[\u@\h \W]\$ '
